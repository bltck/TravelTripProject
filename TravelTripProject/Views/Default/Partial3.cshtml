@using TravelTripProject.Models.Classes
@model List<Blog>

<div class="padding">
    <h4>En Popüler 10 Blog</h4>
    <ul class="list list-pad">
        @{ int i = 0;}
        @foreach (var x in Model)
        {
            <li class="list-bg"><span>@{i++;} @i.</span><a href="/Blog/BlogDetails/@x.Id">@x.Title</a></li>
        }
    </ul>
    <a href="/Blog/Index/" class="link1">Tüm Blog Yazılarını Gör</a>
</div>
